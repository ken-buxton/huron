<h1>Listing Aggregate Details</h1>

<div class="scf">
<table>
  <tr>
    <th></th>
    <% if is_admin_role then%>
      <th></th>
      <th></th>
    <% end %>
    <th>Aggregate table name</th>
    <th>Agg dim table</th>
    <th>Order</th>
    <th>Parent Definition</th>
  </tr>

<% @aggregate_details.each do |aggregate_detail| %>
  <tr>
    <td><%= link_to 'Show', aggregate_detail %></td>
    <% if is_admin_role then%>
      <td><%= link_to 'Edit', edit_aggregate_detail_path(aggregate_detail) %></td>
      <td><%= link_to 'Destroy', aggregate_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    <% end %>
    <td><%= aggregate_detail.aggregate_table_name %></td>
    <td><%= aggregate_detail.agg_dim_table %></td>
    <td><%= aggregate_detail.order %></td>
    <td><%= aggregate_detail.parent_def %></td>
  </tr>
<% end %>
</table>
</div>

<br />

<%= link_to 'New Aggregate detail', new_aggregate_detail_path %>
